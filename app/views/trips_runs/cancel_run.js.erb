<% if @run %>
  var run_id = <%= @run.id %>;
  $('#run_trips_panel_' + run_id).replaceWith("<%= j render('trips_runs/run_trips_panel', run: @run) %>");
  $('#runs_panel table tbody tr[data-run-id=' + run_id + '] td.trips_count').html(0);
  $('#unassigned_trips_panel').replaceWith("<%= j render('trips_runs/unassigned_trips_panel') %>");
  configure_unassigned_trips_table();
<% end %>